{
   "action": {
      "default_icon": {
         "48": "img/icon-48.png"
      },
      "default_popup": "options.html?p"
   },
   "author": "Lauri Rooden",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Fast JSON viewer - highlights, shows items count/size, handles large files",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "img/icon-128.png",
      "32": "img/icon-32.png",
      "48": "img/icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "JSON Lite",
   "options_page": "options.html",
   "options_ui": {
      "page": "options.html"
   },
   "permissions": [ "contextMenus", "storage", "activeTab", "tabs", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "23.5.0"
}
